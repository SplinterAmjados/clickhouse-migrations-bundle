{
    "name": "dmamontov/clickhouse-migrations-bundle",
    "type": "symfony-bundle",
    "description": "Symfony ClickhouseMigrationsBundle",
    "keywords": ["Clickhouse", "Migrations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Konnov",
            "email": "a.konnov73@bk.ru"
        },
        {
            "name": "Dmitry Mamontov",
            "email": "d@mamontov.tech"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "smi2/phpclickhouse": "^1.4",
        "symfony/console": "*",
        "symfony/http-kernel": "*"
    },
    "autoload": {
        "psr-4": {
            "Nalogka\\ClickhouseMigrationsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://pkgs.nalogka.com"
        }
    ]
}
